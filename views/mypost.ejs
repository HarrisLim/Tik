<!DOCTYPE html>
<html>
	<head>
		<%- include header.ejs %>
		<script>
			window.onload = function() {
				<%- include searchTag.js%>
			}
		</script>

		<title>My post</title>
	</head>
	<body>
		<%- include rightTop.ejs %>
		<br>
		<% if(results.length === 0) {%>
			<div class="ui container" style="font-size:20px;text-align:center">You don't have any posting. :-(</div> <br>
		<% } else {%>
			<div class="ui two column grid" style="padding:0 10% 1%">
				<%- include pagination.ejs%>
			</div>
		<% } %>
		<br>
		<div style="text-align:center;padding:3% 0 10% 0">
				<% 
					for(var j=0; j<results.length/4; j++) {
				%>
					<a href="/process/mypost/<%= j+1%>" style="padding:5%"><%= j+1%></a>&emsp;
				<%
					}
				%>
		</div>
	</body>
</html>